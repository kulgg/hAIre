---
import Layout from "./Layout.astro";
import MenuItems from "./MenuItems.astro";

export interface Props {
	stakeholder: string;
}

const { stakeholder } = Astro.props;
const lowerCaseStakeholder = stakeholder.toLocaleLowerCase();
---

<Layout link={`/${lowerCaseStakeholder}`}>
	<MenuItems stakeholder={stakeholder} slot="menu-items" />
	<slot />
</Layout>
